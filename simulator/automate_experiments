#    time python3 main.py --N_TRANSACTIONS $i --TOTAL_INVESTMENT $j > "experiment_results/100nodes_6hr_CDS_no_loops/N_TRANSACTIONS_${i}_TOTAL_INVESTMENT_${j}.log"
#    time python3 main.py --N_TRANSACTIONS $i --TOTAL_INVESTMENT $j > "experiment_results/200nodes_6hr_CDS_no_loops/N_TRANSACTIONS_${i}_TOTAL_INVESTMENT_${j}.log"
#    time python3 main.py --N_TRANSACTIONS $i --TOTAL_INVESTMENT $j > "experiment_results/300nodes_6hr_CDS_no_loops/N_TRANSACTIONS_${i}_TOTAL_INVESTMENT_${j}.log"
#    time python3 main.py --N_TRANSACTIONS $i --TOTAL_INVESTMENT $j > "experiment_results/100nodes_6hr_UST/N_TRANSACTIONS_${i}_TOTAL_INVESTMENT_${j}.log"
#    time python3 main.py --N_TRANSACTIONS $i --TOTAL_INVESTMENT $j > "experiment_results/200nodes_6hr_UST/N_TRANSACTIONS_${i}_TOTAL_INVESTMENT_${j}.log"
